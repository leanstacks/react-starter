{
  "name": "react-starter",
  "description": "A starter project for React applications using a curated technology stack for optimal performance, reliability, and maintainability.",
  "keywords": [
    "react, typescript, vite"
  ],
  "author": "LeanStacks",
  "license": "MIT",
  "version": "1.0.0-alpha.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leanstacks/react-starter.git"
  },
  "scripts": {
    "clean": "rimraf coverage dist storybook-static",
    "dev": "vite --open",
    "build": "tsc && vite build",
    "build:storybook": "storybook build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --max-warnings 0",
    "prepare": "husky",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest --coverage --coverage.all=false",
    "test:ci": "vitest run --coverage --silent",
    "test:ui": "vitest --ui --coverage --silent"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "7.1.0",
    "@fortawesome/free-regular-svg-icons": "7.1.0",
    "@fortawesome/free-solid-svg-icons": "7.1.0",
    "@fortawesome/react-fontawesome": "3.2.0",
    "@hookform/resolvers": "5.2.2",
    "@react-spring/web": "10.0.3",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-query": "5.90.20",
    "@tanstack/react-query-devtools": "5.91.3",
    "@tanstack/react-table": "8.21.3",
    "axios": "1.13.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dayjs": "1.11.19",
    "i18next": "25.8.4",
    "i18next-browser-languagedetector": "8.2.0",
    "lodash": "4.17.23",
    "qs": "6.14.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "7.71.1",
    "react-i18next": "16.5.4",
    "react-router-dom": "7.13.0",
    "react-syntax-highlighter": "16.1.0",
    "recharts": "3.7.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "uuid": "13.0.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "5.0.0",
    "@eslint/js": "9.39.2",
    "@storybook/addon-docs": "10.2.7",
    "@storybook/addon-onboarding": "10.2.7",
    "@storybook/addon-themes": "10.2.7",
    "@storybook/react-vite": "10.2.7",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@testing-library/user-event": "14.6.1",
    "@types/eslint__js": "8.42.3",
    "@types/lodash": "4.17.23",
    "@types/qs": "6.14.0",
    "@types/react": "19.2.13",
    "@types/react-dom": "19.2.3",
    "@types/react-syntax-highlighter": "15.5.13",
    "@types/uuid": "10.0.0",
    "@vitejs/plugin-react": "5.1.3",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "4.0.18",
    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.5.0",
    "eslint-plugin-storybook": "10.2.7",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "jsdom": "28.0.0",
    "msw": "2.12.9",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "rimraf": "6.1.2",
    "storybook": "10.2.7",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  }
}
